<?xml version="1.0" encoding="windows-1251"?>
<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema">
  <xs:element name="Файл">
    <xs:annotation>
      <xs:documentation>Файл обмена</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:element name="СвУчДокОбор">
          <xs:annotation>
            <xs:documentation>Сведения об участниках электронного документооборота</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element minOccurs="0" name="СвОЭДОтпрСФ">
                <xs:annotation>
                  <xs:documentation>Сведения об операторе электронного документооборота отправителя в рамках электронного документооборота счетов-фактур</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="НаимОрг" use="required">
                    <xs:annotation>
                      <xs:documentation>Наименование</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="1" />
                        <xs:maxLength value="1000" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ИННЮЛ" type="ИННЮЛТип" use="required">
                    <xs:annotation>
                      <xs:documentation>ИНН</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                  <xs:attribute name="ИдЭДОСФ" use="required">
                    <xs:annotation>
                      <xs:documentation>Идентификатор оператора электронного документооборота в рамках электронного документооборота счетов-фактур</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:length value="3" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
            </xs:sequence>
            <xs:attribute name="ИдОтпр" use="required">
              <xs:annotation>
                <xs:documentation>Идентификатор участника документооборота - отправителя файла обмена информации покупателя</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:minLength value="4" />
                  <xs:maxLength value="46" />
                </xs:restriction>
              </xs:simpleType>
            </xs:attribute>
            <xs:attribute name="ИдПол" use="required">
              <xs:annotation>
                <xs:documentation>Идентификатор участника документооборота - получателя файла обмена информации покупателя</xs:documentation>
              </xs:annotation>
              <xs:simpleType>
                <xs:restriction base="xs:string">
                  <xs:minLength value="4" />
                  <xs:maxLength value="46" />
                </xs:restriction>
              </xs:simpleType>
            </xs:attribute>
          </xs:complexType>
        </xs:element>
        <xs:element name="Документ">
          <xs:annotation>
            <xs:documentation>Документ о приемке и (или) расхождениях (информация покупателя)</xs:documentation>
          </xs:annotation>
          <xs:complexType>
            <xs:sequence>
              <xs:element name="ИдентДокПР">
                <xs:annotation>
                  <xs:documentation>Дата и номер документа о приемке и (или) расхождениях</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="НомДокПР" use="required">
                    <xs:annotation>
                      <xs:documentation>Номер документа о приемке и (или) расхождениях</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:string">
                        <xs:minLength value="1" />
                        <xs:maxLength value="255" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ДатаДокПР" type="ДатаТип" use="required">
                    <xs:annotation>
                      <xs:documentation>Дата составления (выписки) документа о приемке и (или) расхождениях</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element minOccurs="0" name="ИспрДокПР">
                <xs:annotation>
                  <xs:documentation>Исправление документа о приемке и (или) расхождениях</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:attribute name="НомИспрДокПР" use="required">
                    <xs:annotation>
                      <xs:documentation>Исправление: №</xs:documentation>
                    </xs:annotation>
                    <xs:simpleType>
                      <xs:restriction base="xs:integer">
                        <xs:totalDigits value="3" />
                        <xs:minInclusive value="1" />
                      </xs:restriction>
                    </xs:simpleType>
                  </xs:attribute>
                  <xs:attribute name="ДатаИспрДокПР" type="ДатаТип" use="required">
                    <xs:annotation>
                      <xs:documentation>Исправление: Дата</xs:documentation>
                    </xs:annotation>
                  </xs:attribute>
                </xs:complexType>
              </xs:element>
              <xs:element name="СодФХЖ1">
                <xs:annotation>
                  <xs:documentation>Содержание события (факта хозяйственной жизни (1)) - сведения об обстоятельствах приемки</xs:documentation>
                </xs:annotation>
                <xs:complexType>
                  <xs:sequence>
                    <xs:element name="Продавец" type="УчастникТип">
                      <xs:annotation>
                        <xs:documentation>Продавец (поставщик, исполнитель)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element name="Покупатель" type="УчастникТип">
                      <xs:annotation>
                        <xs:documentation>Покупатель (заказчик)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="Грузоотправитель" type="УчастникТип">
                      <xs:annotation>
                        <xs:documentation>Грузоотправитель (отправитель)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="Грузополучатель" type="УчастникТип">
                      <xs:annotation>
                        <xs:documentation>Грузополучатель (получатель)</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="СтрахКом" type="УчастникТип">
                      <xs:annotation>
                        <xs:documentation>Страховая компания</xs:documentation>
                      </xs:annotation>
                    </xs:element>
                    <xs:element minOccurs="0" name="Приказ">
                      <xs:annotation>
                        <xs:documentation>Дата и номер приказа (распоряжения) о назначении комиссии</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:attribute name="ДатаПрик" type="ДатаТип" use="required">
                          <xs:annotation>
                            <xs:documentation>Дата приказа (распоряжения)</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="НомПрик" use="required">
                          <xs:annotation>
                            <xs:documentation>Номер приказа (распоряжения)</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="255" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="СвОсмГруз">
                      <xs:annotation>
                        <xs:documentation>Сведения о событиях, связанных с осмотром груза (о результатах осмотра прибывшего груза)</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="0" maxOccurs="unbounded" name="СертСоотв">
                            <xs:annotation>
                              <xs:documentation>Номер сертификата соответствия</xs:documentation>
                            </xs:annotation>
                            <xs:simpleType>
                              <xs:restriction base="xs:string">
                                <xs:minLength value="1" />
                                <xs:maxLength value="50" />
                              </xs:restriction>
                            </xs:simpleType>
                          </xs:element>
                          <xs:element name="СопрДок" type="СопрДокТип">
                            <xs:annotation>
                              <xs:documentation>Наименование, номер и дата сопроводительного документа (идентификатор сопроводительного документа)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element minOccurs="0" name="ИнфПолСвОсм" type="ИнфПолТип">
                            <xs:annotation>
                              <xs:documentation>Информационное поле сведений о результатах осмотра</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="ДатаОсм" type="ДатаТип" use="optional">
                          <xs:annotation>
                            <xs:documentation>Дата осмотра прибывшего  груза</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="МестоСост" use="optional">
                          <xs:annotation>
                            <xs:documentation>Место составления документа о приемке и (или) расхождениях</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="100" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="ВремяНач" type="ВремяТип" use="optional">
                          <xs:annotation>
                            <xs:documentation>Время начала приемки</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ВремяОконч" type="ВремяТип" use="optional">
                          <xs:annotation>
                            <xs:documentation>Время окончания приемки</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ДатаОтпр" type="ДатаТип" use="optional">
                          <xs:annotation>
                            <xs:documentation>Дата отправки груза со станции (пристани, порта)</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" maxOccurs="unbounded" name="СвСопрДок">
                      <xs:annotation>
                        <xs:documentation>Сведения о грузе по сопроводительным транспортным документам</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="0" name="МассаДок" type="МасГрузТип">
                            <xs:annotation>
                              <xs:documentation>Масса в пункте отправления (по документам грузоотправителя)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element minOccurs="0" name="МассаФакт" type="МасГрузТип">
                            <xs:annotation>
                              <xs:documentation>Масса в пункте прибытия</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element minOccurs="0" name="ИнфПолСопрДок" type="ТекстИнфТип">
                            <xs:annotation>
                              <xs:documentation>Информационное поле сведений о грузе по сопроводительным транспортным документам (в том числе для указания особых отметок)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="ОтмПломб" use="optional">
                          <xs:annotation>
                            <xs:documentation>Отметка об опломбировании</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="20" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="ВидУпак" use="optional">
                          <xs:annotation>
                            <xs:documentation>Вид упаковки</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="100" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="КолМест" use="optional">
                          <xs:annotation>
                            <xs:documentation>Количество мест</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:integer">
                              <xs:totalDigits value="12" />
                              <xs:minInclusive value="0" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="НаимГруз" use="required">
                          <xs:annotation>
                            <xs:documentation>Наименование груза (товара)</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="1000" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="ОКЕИ_Тов" type="ОКЕИТип" use="optional">
                          <xs:annotation>
                            <xs:documentation>Код единицы измерения</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="НаимЕдИзм" use="optional">
                          <xs:annotation>
                            <xs:documentation>Наименование единицы измерения</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="255" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="СвВремПрием">
                      <xs:annotation>
                        <xs:documentation>Сведения о дате и времени событий, связанных с приемкой груза</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:attribute name="ПрибГруз" type="ДатаВремяТип" use="optional">
                          <xs:annotation>
                            <xs:documentation>Прибытие на место назначения (станция, пристань, порт)</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ВыдачГруз" type="ДатаВремяТип" use="optional">
                          <xs:annotation>
                            <xs:documentation>Выдача груза транспортной организацией</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ВскрытГруз" type="ДатаВремяТип" use="optional">
                          <xs:annotation>
                            <xs:documentation>Вскрытие вагона (других транспортных средств)</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                        <xs:attribute name="ДостГруз" type="ДатаВремяТип" use="optional">
                          <xs:annotation>
                            <xs:documentation>Доставка на склад получателя</xs:documentation>
                          </xs:annotation>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="ДрОбстПрием">
                      <xs:annotation>
                        <xs:documentation>Другие обстоятельства приемки ценностей</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:sequence>
                          <xs:element minOccurs="0" name="Перевозчик" type="УчастникТип">
                            <xs:annotation>
                              <xs:documentation>Перевозчик ценностей (груза), представитель которого передает груз</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                          <xs:element minOccurs="0" name="СопрТрансДок" type="СопрДокТип">
                            <xs:annotation>
                              <xs:documentation>Наименование, номер и дата сопроводительного транспортного документа (идентификатор сопроводительного транспортного документа)</xs:documentation>
                            </xs:annotation>
                          </xs:element>
                        </xs:sequence>
                        <xs:attribute name="УсловХран" use="optional">
                          <xs:annotation>
                            <xs:documentation>Условия хранения ценностей на складе получателя</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="1000" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="СостТара" use="optional">
                          <xs:annotation>
                            <xs:documentation>Состояние тары и упаковки в момент осмотра ценностей</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="1000" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="ОпредНедост" use="optional">
                          <xs:annotation>
                            <xs:documentation>Тип определения недостающих ценностей</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="200" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                        <xs:attribute name="ДопСв" use="optional">
                          <xs:annotation>
                            <xs:documentation>Дополнительные сведения</xs:documentation>
                          </xs:annotation>
                          <xs:simpleType>
                            <xs:restriction base="xs:string">
                              <xs:minLength value="1" />
                              <xs:maxLength value="1000" />
                            </xs:restriction>
                          </xs:simpleType>
                        </xs:attribute>
                      </xs:complexType>
                    </xs:element>
                    <xs:element minOccurs="0" name="СвЛицПрин">
                      <xs:annotation>
                        <xs:documentation>Сведения о лице, принявшем товар (получившем груз) (в том числе на ответственное хранение)</xs:documentation>
                      </xs:annotation>
                      <xs:complexType>
                        <xs:choice>
                          <xs:element name="РабОргПок">
                            <xs:annotation>
                              <xs:documentation>Работник организации покупателя</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:sequence>
                                <xs:element name="ФИО" type="ФИОТип">
                                  <xs:annotation>
                                    <xs:documentation>Фамилия, имя, отчество</xs:documentation>
                                  </xs:annotation>
                                </xs:element>
                              </xs:sequence>
                              <xs:attribute name="Должность" use="required">
                                <xs:annotation>
                                  <xs:documentation>Должность</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:minLength value="1" />
                                    <xs:maxLength value="128" />
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:attribute>
                              <xs:attribute name="ИныеСвед" use="optional">
                                <xs:annotation>
                                  <xs:documentation>Иные сведения, идентифицирующие физическое лицо</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:minLength value="1" />
                                    <xs:maxLength value="255" />
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:attribute>
                              <xs:attribute name="ОснПолн" use="required">
                                <xs:annotation>
                                  <xs:documentation>Основание полномочий (доверия)</xs:documentation>
                                </xs:annotation>
                                <xs:simpleType>
                                  <xs:restriction base="xs:string">
                                    <xs:minLength value="1" />
                                    <xs:maxLength value="120" />
                                  </xs:restriction>
                                </xs:simpleType>
                              </xs:attribute>
                            </xs:complexType>
                          </xs:element>
                          <xs:element name="ИнЛицо">
                            <xs:annotation>
                              <xs:documentation>Иное лицо</xs:documentation>
                            </xs:annotation>
                            <xs:complexType>
                              <xs:choice>
                                <xs:element name="ПредОргПрин">
                                  <xs:annotation>
                                    <xs:documentation>Представитель организации, уполномоченной на принятие товаров (груза)</xs:documentation>
                                  </xs:annotation>
                                  <xs:complexType>
                                    <xs:sequence>
                                      <xs:element name="ФИО" type="ФИОТип">
                                        <xs:annotation>
                                          <xs:documentation>Фамилия, имя, отчество</xs:documentation>
                                        </xs:annotation>
                                      </xs:element>
                                    </xs:sequence>
                                    <xs:attribute name="Должность" use="required">
                                      <xs:annotation>
                                        <xs:documentation>Должность</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                          <xs:minLength value="1" />
                                          <xs:maxLength value="128" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="ИныеСвед" use="optional">
                                      <xs:annotation>
                                        <xs:documentation>Иные сведения, идентифицирующие физическое лицо</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                          <xs:minLength value="1" />
                                          <xs:maxLength value="255" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="НаимОргПрин" use="required">
                                      <xs:annotation>
                                        <xs:documentation>Наименование организации</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                          <xs:minLength value="1" />
                                          <xs:maxLength value="128" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="ОснДоверОргПрин" use="optional">
                                      <xs:annotation>
                                        <xs:documentation>Основание, по которому организация уполномочена на принятие товаров (груза)</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                          <xs:minLength value="1" />
                                          <xs:maxLength value="120" />
                                        </xs:restriction>
                                      </xs:simpleType>
                                    </xs:attribute>
                                    <xs:attribute name="ОснПолнПредПрин" use="optional">
                                      <xs:annotation>
                                        <xs:documentation>Основание полномочий представителя организации на принятие товаров (груза)</xs:documentation>
                                      </xs:annotation>
                                      <xs:simpleType>
                                        <xs:restriction base="xs:string">
                                          <xs:minLength value="1" />
                                          <xs:maxLength value="120" />
