										<xs:element name="ВсегоОпл">
											<xs:annotation>
												<!---************************************************************************************************************************************************************-->
												<xs:appinfo>
													<sch:pattern name="Проверка обязательности присутствия элемента СтТовБезНДСВсего в зависимости от значения элемента Функция и значения элемента СпОбстФСЧФ">
														<sch:rule context="СведТов">
															<sch:assert test="usch:iif(
												( ( ../../@Функция='СЧФ' )  or  ( ../../@Функция='СЧФДОП' ) ) and (../../СвСчФакт/ДопСвФХЖ1/@СпОбстФСЧФ!=2) , 
												count(@СтТовБезНДСВсего)!=0, 
